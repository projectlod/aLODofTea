  <rdf:Description rdf:about="http://example.org/bookoftea/person/Confucius">
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <foaf:name>Confucius</foaf:name>
    <skos:note>Chinese philosopher and teacher, active ca</skos:note>
    <schema1:birthDate rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">0401</schema1:birthDate>
    <schema1:deathDate rdf:datatype="http://www.w3.org/2001/XMLSchema#gYear">0600</schema1:deathDate>
