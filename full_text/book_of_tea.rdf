    <schema1:hasPart rdf:resource="http://example.org/bookoftea/paragraph/3"/>
    <schema1:hasPart rdf:resource="http://example.org/bookoftea/paragraph/4"/>
    <schema1:hasPart rdf:resource="http://example.org/bookoftea/paragraph/5"/>
